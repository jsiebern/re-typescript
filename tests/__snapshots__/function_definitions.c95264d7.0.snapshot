module SomeFunction_a: {
  type t;
  let string: string => t;
  let number: float => t;
} = {
  [@unboxed]
  type t =
    | Any('a): t;
  let string = (v: string) => Any(v);
  let number = (v: float) => Any(v);
};
type someFunction_a = SomeFunction_a.t;
type someFunction_b = {inline: float}
