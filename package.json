{
  "name": "re-typescript",
  "version": "1.0.0",
  "main": "dist/index.html",
  "license": "MIT",
  "scripts": {
    "bs:build": "bsb -make-world",
    "bs:clean": "bsb -clean-world",
    "start": "parcel ./src_js/index.html",
    "assets": "./compile-assets.sh",
    "docs": "rm docs/*.js*; parcel build src_js/index.html --target browser"
  },
  "source": [
    "src_js/index.html"
  ],
  "browser": "docs/index.html",
  "targets": {
    "browser": {
      "engines": {
        "browsers": [
          "last 1 version"
        ]
      },
      "distDir": "docs",
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "bs-platform": "^7.3.2",
    "parcel": "^2.0.0-alpha.3.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reason": "^3.3.4",
    "reason-react": "^0.8.0",
    "tablecloth-bucklescript": "^0.0.7"
  }
}
